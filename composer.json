{
    "name": "gopaljha/laravel-sqs-datadog",
    "description": "A simple package to wrap DataDog API for Timeseries metrics using the CURL, SQS, GuzzleHttp",
    "type": "package",
    "keywords": [
        "laravel",
        "datadog"
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "GopalJha",
            "email": "gopalpsdjha89@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "GopalJha\\LaravelSQSDataDog\\LaravelSQSDataDogServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "GopalJha\\LaravelSQSDataDog\\": "src/"
        }
    }
}
